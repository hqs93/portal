{
  "name": "mocs-portal",
  "version": "0.0.0",
  "scripts": {
    "test": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vite build --mode buildDev",
    "build:prod": "vite build --mode buildProd",
    "build:test": "vite build --mode buildTest",
    "cz:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "dev": "vite",
    "preview": "vite preview",
    "serve:dev": "vite serve --mode serveDev",
    "serve:prod": "vite serve --mode serveProd",
    "serve:test": "vite serve --mode serveTest",
    "prepare": "husky install"
  },
  "dependencies": {
    "ant-design-vue": "^1.7.8",
    "normalize.css": "^8.0.1",
    "qs": "^6.10.3",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@ziyi2/vue-cli-plugin-ui-cz": "~1.0.0",
    "autoprefixer": "^10.4.2",
    "commitizen": "^3.0.5",
    "conventional-changelog": "^3.0.5",
    "cz-customizable": "^5.2.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "unplugin-vue-components": "^0.17.14",
    "vite": "^2.7.2",
    "vite-plugin-compression": "^0.4.0",
    "vite-plugin-pwa": "^0.11.12",
    "vite-plugin-style-import": "^1.4.1",
    "vue-tsc": "^0.29.8"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT"
}
